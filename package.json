{
  "name": "react-keystone",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "alt": "^0.17.1",
    "async": "^0.9.0",
    "babel": "^5.8.12",
    "dotenv": "^1.1.0",
    "express": "^4.13.0",
    "fluxxor": "^1.6.0",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "keystone": "^0.3.11",
    "keystone-utils": "^0.3.0",
    "lodash": "^3.10.0",
    "material-ui": "^0.10.0",
    "materialize-css": "^0.96.1",
    "node-jsx": "^0.13.3",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-engine": "^1.7.0",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "restful-keystone": "^0.3.0",
    "superagent": "^1.2.0",
    "underscore": "^1.8.0",
    "underscore.string": "^3.1.1",
    "update-notifier": "^0.5.0",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "jshint-stylish": "^0.1.3",
    "less": "^2.5.1",
    "nodemon": "^1.4.0",
    "npm-run-all": "^1.2.6",
    "parallelshell": "^2.0.0",
    "watch-less": "0.0.4",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.10.22",
    "npm": ">=1.3.14"
  },
  "scripts": {
    "styles:build": "lessc public/styles/main.less > public/build/main.css",
    "styles:watch": "watch-less --compress public/styles/main.less --output public/build --directory public/styles/ --extension .css",
    "webpack:build": "webpack --progress --colors -p",
    "webpack:watch": "webpack --progress --colors -d --watch",
    "backend:start": "babel-node keystone.js",
    "backend:watch": "nodemon --watch public/routes.jsx --watch routes --watch models --watch keystone.js --exec babel-node keystone.js",
    "start": "npm run webpack:build; npm run styles:build; npm run backend:start",
    "dev": "parallelshell 'npm run webpack:watch' 'npm run backend:watch' 'npm run styles:watch'"
  },
  "main": "keystone.js"
}
