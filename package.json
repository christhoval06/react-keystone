{
  "name": "react-keystone",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "async": "^0.9.0",
    "dotenv": "^1.1.0",
    "express": "^4.13.0",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "keystone": "^0.3.11",
    "materialize-css": "^0.96.1",
    "node-jsx": "^0.13.3",
    "node-libs-browser": "^0.5.2",
    "nodemon": "^1.3.7",
    "react": "^0.13.3",
    "react-engine": "^1.7.0",
    "react-router": "^0.13.3",
    "underscore": "^1.8.0",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "gulp": "^3.7.0",
    "gulp-jshint": "^1.9.0",
    "gulp-watch": "^0.6.5",
    "jshint-stylish": "^0.1.3",
    "less": "^2.5.1",
    "nodeman": "^1.1.2",
    "watch-less": "0.0.4",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.10.22",
    "npm": ">=1.3.14"
  },
  "scripts": {
    "less:build": "lessc public/styles/main.less > public/build/main.css",
    "less:watch": "watch-less --compress public/styles/main.less --output public/build --directory public/styles/ --extension .css",
    "webpack:build": "webpack --progress --colors",
    "webpack:watch": "webpack --progress --colors --watch",
    "backend": "node keystone.js",
    "start": "npm run webpack:build; npm run less:build; npm run backend",
    "dev": "npm run backend & wepback:watch & npm run less:watch"
  },
  "main": "keystone.js"
}
