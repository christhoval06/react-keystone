    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.20",
    "babel-loader": "^5.3.2",
    "browser-sync": "^2.8.2",
    "bs-snippet-injector": "git+https://github.com/ksespinola/bs-snippet-injector.git",
    "jshint-stylish": "^0.1.3",
    "less": "^2.5.1",
    "minimist": "^1.1.2",
    "nodemon": "^1.4.0",
    "npm-run-all": "^1.2.6",
    "parallelshell": "^2.0.0",
    "watch-less": "0.0.4",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.10.22",
    "npm": ">=1.3.14"
  },
  "scripts": {
    "styles:build": "lessc public/styles/main.less > public/build/main.css",
    "styles:watch": "watch-less --compress public/styles/main.less --output public/build --directory public/styles/ --extension .css",
    "webpack:build": "webpack --progress --colors -p",
    "webpack:watch": "webpack -d --progress --colors --watch  --inline",
    "backend:start": "babel-node keystone.js",
    "backend:watch": "nodemon --debug --ignore public/fonts --ignore public/images --ignore node_modules --exec babel-node keystone.js",
    "start": "npm run webpack:build; npm run styles:build; npm run backend:start",
    "dev": "parallelshell 'npm run webpack:watch' 'npm run backend:watch' 'npm run styles:watch'"
  },
  "main": "keystone.js"
}
