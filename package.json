  "scripts": {
    "packages:update": "ncu -u",
    "styles:build": "node-sass public/styles/main.sass -o public/build",
    "styles:watch": "node-sass -w public/styles/main.sass -o public/build --source-map-embed",
    "webpack:build": "webpack --progress --colors -p",
    "webpack:watch": "webpack -d --progress --colors --watch  --inline",
    "backend:start": "babel-node keystone.js",
    "backend:watch": "nodemon --debug --ignore public/fonts --ignore public/images --ignore public/views --ignore node_modules --exec babel-node keystone.js",
