    "dotenv": "^1.2.0",
    "express": "^4.13.0",
    "fluxxor": "^1.6.0",
    "immutable": "^3.7.4",
    "js": "^0.1.0",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "keystone": "^0.3.11",
    "keystone-utils": "^0.3.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.6",
    "node-jsx": "^0.13.3",
    "node-libs-browser": "^0.5.2",
    "normalize.css": "^3.0.3",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.4",
    "react-container": "^0.2.4",
    "react-engine": "^1.7.0",
    "react-router": "^0.13.3",
    "react-router-bootstrap": "^0.18.1",
    "react-wysiwyg-editor": "0.0.6",
